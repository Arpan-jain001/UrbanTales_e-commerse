#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/UrbanTales-Website/Frontend/node_modules/.pnpm/vite@7.2.2_@types+node@24.1_e1c008ce0a27267495efe746bf7a105e/node_modules/vite/bin/node_modules:/mnt/d/UrbanTales-Website/Frontend/node_modules/.pnpm/vite@7.2.2_@types+node@24.1_e1c008ce0a27267495efe746bf7a105e/node_modules/vite/node_modules:/mnt/d/UrbanTales-Website/Frontend/node_modules/.pnpm/vite@7.2.2_@types+node@24.1_e1c008ce0a27267495efe746bf7a105e/node_modules:/mnt/d/UrbanTales-Website/Frontend/node_modules/.pnpm/node_modules:/mnt/d/UrbanTales-Website/Frontend/node_modules:/mnt/d/UrbanTales-Website/node_modules:/mnt/d/node_modules:/mnt/d/UrbanTales-Website/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/UrbanTales-Website/Frontend/node_modules/.pnpm/vite@7.2.2_@types+node@24.1_e1c008ce0a27267495efe746bf7a105e/node_modules/vite/bin/node_modules:/mnt/d/UrbanTales-Website/Frontend/node_modules/.pnpm/vite@7.2.2_@types+node@24.1_e1c008ce0a27267495efe746bf7a105e/node_modules/vite/node_modules:/mnt/d/UrbanTales-Website/Frontend/node_modules/.pnpm/vite@7.2.2_@types+node@24.1_e1c008ce0a27267495efe746bf7a105e/node_modules:/mnt/d/UrbanTales-Website/Frontend/node_modules/.pnpm/node_modules:/mnt/d/UrbanTales-Website/Frontend/node_modules:/mnt/d/UrbanTales-Website/node_modules:/mnt/d/node_modules:/mnt/d/UrbanTales-Website/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
